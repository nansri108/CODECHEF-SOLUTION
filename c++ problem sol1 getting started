#include <bits/stdc++.h>
using namespace std;

int main() 
{
 int A, B, C, D, E, F, G, H, I;
 cin>>A>>B;
 cin>>C>>E;
 cin>>H>>I;
 cout<<A<<" "<<B<<" "<<C<<" "<<D<<" "<<E<<" "<<F<<" "<<G<<" "<<H<<" "<<I;
  return 0;
}
