#include <iostream>
using namespace std;

int main() {
  int n;
  cin >> n;
  for (int i = 0 ; i < n ; i = i + 1) {
    cout << "C++"<< endl;
  }
  return 0;
}
......................
#include <iostream>
using namespace std;

int main()
{
    int n;
    cin >> n;

    for(int i = 1; i <=10; i = i+1) {
        cout << n << " x " << i << " = " << n*i<< endl;
    }
    
    return 0;
}
............................................
#include <iostream>
using namespace std;
int main()
{
    int num;
    int fact = 1;
    cin >> num;
    for(int a = 1; a <= num; a = a + 1)
    {
        fact = fact*a;
    }
	cout<<"The factorial of the given number is: "<<fact<<endl;
    return 0;
}
.....................................
#include <iostream>
using namespace std;

int main() {
  for (int i = 0; i <= 15; i = i+1) {
    if (i == 13) {
      break ;
    }
    cout << i << endl;
  } 
  return 0;
}
......................................
#include <iostream>
#include <string>
using namespace std;

int main() {
  string x[6] = {"C", "O", "D", "I", "N","G"};
  for(int i = 0; i < 6; i=i+1) {
    cout << i << " = " << x[i] << endl;
  }
  return 0;
}
