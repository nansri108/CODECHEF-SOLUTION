#include <iostream>
#include <string>
using namespace std;

int main() {
  int num[ 5 ] = {1, 2, 3, 4,5 };
  cout << num[ 2 ];
  return 0;
}
..........................
#include <iostream>
#include <string>
using namespace std;

int main() {
  string mnts[4] = {"Jan", "Feb", " Dec", "Apr"};
  mnts[ 2 ] = "Mar";
  cout << mnts[2];
  return 0;
}
